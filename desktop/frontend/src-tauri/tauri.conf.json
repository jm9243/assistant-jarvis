{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/2.0.0/tooling/cli/schema.json",
  "productName": "Jarvis Desktop",
  "mainBinaryName": "jarvis-desktop",
  "version": "0.1.0",
  "identifier": "com.jarvis.desktop",
  "build": {
    "beforeDevCommand": "npm run dev -- --host 127.0.0.1 --port 1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:1420"
  },
  "bundle": {
    "active": false
  },
  "app": {
    "windows": [
      {
        "title": "Jarvis Desktop",
        "width": 1280,
        "height": 800,
        "decorations": true,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
